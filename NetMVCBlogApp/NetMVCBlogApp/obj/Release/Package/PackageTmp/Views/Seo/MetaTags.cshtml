@model NetMVCBlogApp.Entity.Options

<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="apple-mobile-web-app-capable" content="yes" />
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="format-detection" content="telephone=no" />
<meta name="referrer" content="always">
<META name="author" content="yusufyilmaz.org">
<META name="Copyright" content="yusufyilmaz.org Tüm hakları saklıdır.">
<meta name="content-language" content="tr-TR" />
<meta name="revisit-after" content="1 day">
<meta name="distribution" content="Global / Local">
<meta name="rating" content="All" />
<meta name="language" content="Turkish" />
<meta name="theme-color" content="#333">
<meta http-equiv="X-UA-Compatible" content="ie=edge">
<meta property="og:type" content="website" />
<meta name="robots" content="index, follow">
<link rel="canonical" href="@Request.Url" />
<meta name="keywords" content="@Model.Tags" />
<meta name="description" content="@Model.Description">
<meta name="twitter:title" content="@ViewBag.Title" />
<meta name="twitter:card" content="summary_large_image" />
<meta name="twitter:description" content="@Model.Description" />
<meta name="twitter:image" content="/img/logo.png" />
<meta name="twitter:url" content="@Request.Url" />
<meta property="og:title" content="@ViewBag.Title" />
<meta property="og:image" content="~/Content/img/icon/icon.png" />
<meta property="og:url" content="@Request.Url" />
<meta property="og:site_name" content="@ViewBag.Title" />
<meta property="og:description" content="@Model.Description" />