@model NetMVCBlogApp.Models.AdminDetailsModel

<div class="about-page">
    <div class="about-page-header">
        <h5>About Me</h5>
    </div>
    <div class="about-page-image">
        <img width="350" src="data:image/jpeg;base64,@Convert.ToBase64String(Model.Image)" alt="@Model.Name @Model.LastName about me  profile image">
    </div>
    <div class="about-page-text">
        @Model.AboutMe
    </div>

    <div class="follow-me">
        <h6 class="follow-me-header">
            FOLLOW ME
        </h6>
        @Html.Action("ContactPageSocial", "Home")
    </div>

</div>